<h1><%= current_user.username %>'s students:</h1>
<ul class="list-group-flush">
<% @students.uniq.each do |s| %>
    <li class="list-group-item"><%= link_to s.name, student_path(s.id) %><br>
    <%= button_to "Delete Student", {controller: :students, action: :destroy, id: s.id}, method: :delete, data: {confirm: "Action cannot be undone, are you sure?"} %><br>
    </li>

<% end %>
</ul>

 <% @courses.each do |course| %>
        <li>
        <%= link_to course.name, course_path(course) %><br>
        <%= course.description %><br> Student: <%= course.student.name %><br>
        <%= link_to "Edit Course", edit_user_course_path(@user, course) %>
        <%= button_to "Delete Course", {controller: :courses, action: :destroy, id: course.id}, method: :delete, data: {confirm: "Action cannot be undone, are you sure?"} %>
        <br>
        </li>
    <% end %>

