<h6>All students:</h6>
<ol>
<% Student.all.uniq.each do |s| %>
    <li><%= link_to s.name, student_path(s.id) %><br>
    <%= button_to "Delete Student", {controller: :students, action: :destroy, id: s.id}, method: :delete %><br>
    </li>

<% end %>
</ol>
