<h1>New Course</h1>
<%= form_with model: @course do |f| %>
    <h3>Name: <%= f.text_field :name %></h3>
    <h3>Description: <%= f.text_field :description %></h3>
    <h3>Student ID: <%= f.text_field :student_id %></h3>
    <h3>User ID: <%= f.text_field :user_id %></h3>
  <%= f.submit "Create Course" %>
<% end %>